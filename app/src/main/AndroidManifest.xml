<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="innovvapp.hack.tourism">
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/padharosa"
        android:label="@string/app_name"
        android:roundIcon="@drawable/padharosa"
        android:supportsRtl="true"
        android:theme="@style/AppThemeBlue">
        <activity
            android:name="innovvapp.hack.tourism.Activities.startActvity"
            android:label="@string/app_title"
            android:theme="@style/AppThemeBlue.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="innovvapp.hack.tourism.Activities.loginActivity"
            android:theme="@style/AppThemeBlue.NoActionBar"></activity>
        <activity android:name="innovvapp.hack.tourism.Activities.MainActivity"
            android:theme="@style/AppThemeBlue.NoActionBar"
            ></activity>
        <activity android:name="innovvapp.hack.tourism.Activities.ActivityFullScreen"
            android:theme="@style/AppThemeBlue.NoActionBar"></activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>